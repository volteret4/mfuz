/* Remove borders from all frames */
QFrame, QGroupBox {
    border: none;
    border-radius: 4px;
}

/* Text input fields */
QLineEdit, QTextEdit {
    border: 1px solid #414868;
    border-radius: 4px;
    padding: 8px;
    background-color: #24283b;
}
/* Buttons */
QPushButton {{
	background-color: {theme['bg']};
    color: {theme['fg']};
    border: none;
    border-radius: 4px;
    padding: 8px 16px;
    font-weight: bold;
}}

QPushButton:hover {
    background-color: #535d8c;
}

QPushButton:pressed {
    background-color: #7aa2f7;
    color: #1a1b26;
}

/* Lists and Trees */
QTreeWidget, QListWidget {
    background-color: #24283b;
    border: none;
    border-radius: 4px;
}

QTreeWidget::item, QListWidget::item {
    padding: 4px;
}

QTreeWidget::item:selected, QListWidget::item:selected {
    background-color: #364A82;
}

/* Tab Widget */
QTabWidget::pane {
    border: none;
    background-color: #24283b;
    border-radius: 4px;
}

QTabBar::tab {
    background-color: #1a1b26;
    color: #a9b1d6;
    border: none;
    padding: 8px 16px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background-color: #24283b;
    color: #7aa2f7;
}

QTabBar::tab:hover:!selected {
    background-color: #364A82;
}

/* Scroll Areas */
QScrollArea {
    border: none;
}

/* Progress Bar */
QProgressBar {
    border: none;
    background-color: #24283b;
    border-radius: 4px;
    text-align: center;
}

QProgressBar::chunk {
    background-color: #7aa2f7;
    border-radius: 4px;
